#include<stdio.h>
int main()
{
    int t,s,p;
    scanf("%d",&t);
    while(t>0)
    {
        int n,i,a[100];
        scanf("%d",&n);
        p=0;
        for(i=1;i<=n;i++)
        {
            scanf("%d",&a[i]);
        }
        s=a[1];
        for(i=2;i<=n;i++)
        {
            s=s+a[i];
            p=p+s;
        }
        t--;
    }
    printf("%d\n",p);
}
 
 
 
